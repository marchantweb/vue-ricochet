{
  "name": "vue-ricochet",
  "version": "0.0.100",
  "description": "Ricochet is a lightweight JS library that integrates with Vue 3 and provides creative options for object positioning.",
  "license": "MIT",
  "author": "Simon Le Marchant<https://github.com/marchantweb>",
  "main": "src/index.js",
  "scripts": {
    "version-patch": "npm version --no-git-tag-version --no-commit-hooks patch  && git add .",
    "publish-to-npm": "npm publish",
    "update-docs-library": "cd docs/ && npm install vue-ricochet@latest"
  },
  "pre-commit": [
    "version-patch",
    "publish-to-npm",
    "update-docs-library"
  ],
  "keywords": [
    "digital signage",
    "positioning",
    "absolute positioning",
    "vue",
    "ricochet",
    "vue-ricochet",
    "creative programming"
  ],
  "dependencies": {
    "gsap": "^3.0"
  },
  "devDependencies": {
    "pre-commit": "^1.2.2"
  }
}
